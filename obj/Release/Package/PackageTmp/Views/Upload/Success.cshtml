
@{
    ViewBag.Title = "Resultado";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
</head>
<body>
    <div class="container">
        <div name="tabla" style="margin-top:10%;">
            <table class="table">
                <thead>
                    <tr>
                        <td class="text-center"><strong>Marca</strong></td>
                        <td class="text-center"><strong>Modelo</strong></td>
                        <td class="text-center"><strong>Segmento</strong></td>
                        <td class="text-center"><strong>Resultado</strong></td>
                    </tr>
                </thead>
                @foreach (var i in ViewBag.Resultado)
                {
                    <tr>
                        <td class="text-center">@i.Marca</td>
                        <td class="text-center">@i.Modelo</td>
                        <td class="text-center">@i.Segmento</td>
                        <td class="text-center">@i.Resultado</td>
                    </tr>
                }

            </table>
        </div>
    </div>
</body>
</html>
